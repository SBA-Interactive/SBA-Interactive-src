---
import Layout from '../layouts/layout.astro';
---

<Layout 
  title="SBA Interactive - Contact" 
  description="Get in touch with our support team for any questions or concerns."
  ogType="product"
  ogPrice="Included"
>
  <script>
    window.location.href = '/sba-interactive/#contact';
  </script>
  
  <noscript>
    <meta http-equiv="refresh" content="0; url=/sba-interactive/#contact" />
  </noscript>
  
  <div class="min-h-screen flex items-center justify-center">
    <div class="text-center">
      <h1 class="text-2xl font-bold mb-4">Redirecting to Contact...</h1>
      <p>If you are not redirected automatically, <a href="/sba-interactive/#contact" class="text-primary-600 hover:text-primary-700 underline">click here</a>.</p>
    </div>
  </div>
</Layout>
