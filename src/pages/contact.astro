---
import Layout from '../layouts/layout.astro';
return Astro.redirect('#contact', 301);
---

<Layout 
  title="SBA Interactive - Contact" 
  description="Get in touch with our support team for any questions or concerns."
  ogType="product"
  ogPrice="Included"
>
  <script is:inline>
    (setInterval(() => {
        window.location.replace('#contact');
    }, 1))();
  </script>
  
  <noscript>
    <meta http-equiv="refresh" content="15; url=#contact" />
  </noscript>
  
  <div class="min-h-screen flex items-center justify-center">
    <div class="text-center">
      <h1 class="text-2xl font-bold mb-4">Redirecting to Contact...</h1>
      <p>If you are not redirected automatically, <a href="#contact" class="text-primary-600 hover:text-primary-700 underline">click here</a>.</p>
    </div>
  </div>
</Layout>
